                    <ui:MenuItem Header="Box" Command="{Binding CommandLoadExampleAsset}" CommandParameter="Assets/Box/Box.glb" />
                    <ui:MenuItem Header="Helmet" Command="{Binding CommandLoadExampleAsset}" CommandParameter="Assets/Helmet/Helmet.glb" />
                    <ui:MenuItem Header="Lantern" Command="{Binding CommandLoadExampleAsset}" CommandParameter="Assets/Lantern/Lantern.glb" />
