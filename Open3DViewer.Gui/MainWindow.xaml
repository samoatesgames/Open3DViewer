    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="46" />
            <RowDefinition Height="*" />
            <RowDefinition Height="46" />
        </Grid.RowDefinitions>

        <!-- Top Menu Row -->
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="*" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>

            <Menu Grid.Column="0" Background="{StaticResource SolidBackgroundFillColorBaseBrush}">

                <!-- File Menu -->
                <ui:MenuItem Header="File">
                    <ui:MenuItem Header="Open" InputGestureText="Ctrl+O" Icon="{StaticResource Icon.File.Open}" Command="{Binding CommandFileOpen}" />
                    <ui:MenuItem Header="Open Recent" IsEnabled="False">
                        <ui:MenuItem Header="Empty" />
                    </ui:MenuItem>
                    <Separator />
                    <ui:MenuItem Header="Save as" InputGestureText="Ctrl+S" Icon="{StaticResource Icon.File.Save}" Command="{Binding CommandFileSaveAs}" />
                    <ui:MenuItem Header="Export Image" InputGestureText="Ctrl+E" Icon="{StaticResource Icon.File.Export}" Command="{Binding CommandFileExportImage}" />
                    <Separator />
                    <ui:MenuItem Header="Exit" InputGestureText="Alt+F4" Command="{Binding CommandFileExit}" />
                </ui:MenuItem>

                <!-- Edit Menu -->
                <ui:MenuItem Header="Edit">
                    <ui:MenuItem Header="Copy" InputGestureText="Ctrl+C" Icon="{StaticResource Icon.Edit.Copy}" Command="{Binding CommandEditCopy}" />
                    <ui:MenuItem Header="Paste" InputGestureText="Ctrl+V" Icon="{StaticResource Icon.Edit.Paste}" Command="{Binding CommandEditPaste}" />
                </ui:MenuItem>

                <!-- Tools Menu -->
                <ui:MenuItem Header="Tools">
                    <ui:MenuItem Header="Environment &amp; Lighting" InputGestureText="L" IsCheckable="True" IsChecked="True" Command="{Binding CommandToolsEnvironmentAndLighting}" />
                    <ui:MenuItem Header="Stats &amp; Shading" InputGestureText="S" IsCheckable="True" Command="{Binding CommandToolsStatsAndShading}" />
                    <ui:MenuItem Header="Grid &amp; Views" InputGestureText="V" IsCheckable="True" Command="{Binding CommandToolsGridAndViews}" />
                </ui:MenuItem>

                <!-- View Menu -->
                <ui:MenuItem Header="View">
                    <ui:MenuItem Header="Hide All Panels" InputGestureText="F9" Command="{Binding CommandViewHideAllPanels}" />
                    <ui:MenuItem Header="Property Panel" IsCheckable="True" Command="{Binding CommandViewPropertyPanel}" />
                    <ui:MenuItem Header="Animation Panel" IsCheckable="True" Command="{Binding CommandViewAnimationPanel}" />
                    <ui:MenuItem Header="Grid" InputGestureText="G" IsCheckable="True" Command="{Binding CommandViewGrid}" />
                    <Separator />
                    <ui:MenuItem Header="Zoom In" InputGestureText="+" Command="{Binding CommandViewZoomIn}" />
                    <ui:MenuItem Header="Zoom Out" InputGestureText="-" Command="{Binding CommandViewZoomOut}" />
                    <Separator />
                    <ui:MenuItem Header="Reset Camera" InputGestureText="Home" Command="{Binding CommandViewResetCamera}" />
                    <Separator />
                    <ui:MenuItem Header="Shading" IsEnabled="False">
                        <ui:MenuItem Header="Empty" />
                    </ui:MenuItem>
                </ui:MenuItem>

                <!-- Help Menu -->
                <ui:MenuItem Header="Help">
                    <ui:MenuItem Header="Controls" InputGestureText="F1" Icon="{StaticResource Icon.Help.Controls}" Command="{Binding CommandHelpControls}" />
                    <ui:MenuItem Header="Settings" Icon="{StaticResource Icon.Help.Settings}" Command="{Binding CommandHelpSettings}" />
                    <ui:MenuItem Header="Feedback" Icon="{StaticResource Icon.Help.Feedback}" Command="{Binding CommandHelpFeedback}" />
                </ui:MenuItem>
            </Menu>

            <Menu Grid.Column="2" Background="{StaticResource SolidBackgroundFillColorBaseBrush}">
                <ui:MenuItem Header="Example Assets" Icon="{StaticResource Icon.ExampleAssets}">
